{
    "name": "kenzap-factory",
    "version": "1.0.0",
    "author": "Kenzap",
    "description": "A simple vanilla ES6 Node.js template with Rollup bundler",
    "license": "Unlicense",
    "type": "module",
    "scripts": {
        "dev": "nodemon --delay 2 server/server.js",
        "build": "rm -R -f dist; mkdir dist; npm-run-all build:*",
        "build:copy": "copyfiles -e './public/_/**' -e './public/**/*.scss' -e './public/**/*.js' --up 1 './public/**/*' 'dist'; copyfiles --up 1 './public/assets/js/*' 'dist' ",
        "build:scripts": "BABEL_ENV=production rollup -c --bundleConfigAsCjs",
        "build:home": "copyfiles --up 2 './public/home/*' 'dist'"
    },
    "dependencies": {
        "@rollup/plugin-babel": "^6.1.0",
        "@rollup/plugin-commonjs": "^29.0.0",
        "@rollup/plugin-node-resolve": "^16.0.3",
        "@rollup/plugin-terser": "^0.4.4",
        "ali-oss": "^6.1.0",
        "babel": "^6.23.0",
        "connect-livereload": "^0.6.1",
        "cookie-parser": "^1.4.7",
        "copyfiles": "^2.4.1",
        "cors": "^2.8.5",
        "dotenv": "^17.2.0",
        "express": "^4.18.2",
        "express-rate-limit": "^8.0.1",
        "express-validator": "^7.2.1",
        "formidable": "^3.5.4",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "jspdf-html2canvas": "^1.5.2",
        "livereload": "^0.9.3",
        "mime-types": "^3.0.1",
        "multer": "^2.0.2",
        "node-cron": "^4.2.1",
        "nodemailer": "^7.0.5",
        "npm-run-all": "^4.1.5",
        "pdfkit": "^0.17.1",
        "pg": "^8.16.3",
        "playwright": "^1.54.2",
        "redis": "^5.6.0",
        "rollup": "^4.57.0",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-livereload": "^2.0.5",
        "rollup-plugin-replace-html-vars": "^1.0.3",
        "sharp": "^0.34.3",
        "three": "^0.182.0",
        "uuid": "^11.1.0",
        "ws": "^8.19.0"
    },
    "devDependencies": {
        "esbuild": "^0.25.8",
        "nodemon": "^3.1.10"
    }
}
